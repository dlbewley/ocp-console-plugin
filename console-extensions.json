[
    {
        "type": "console.navigation/section",
        "properties": {
            "id": "example-section",
            "perspective": "admin",
            "name": "Example"
        }
    },
    {
        "type": "console.page/route",
        "properties": {
            "path": "/example/page",
            "component": {
                "$codeRef": "ExamplePage"
            }
        }
    },
    {
        "type": "console.navigation/href",
        "properties": {
            "id": "example-page-nav",
            "perspective": "admin",
            "section": "example-section",
            "name": "Example Plugin Page",
            "href": "/example/page"
        }
    },
    {
        "type": "console.page/route",
        "properties": {
            "path": "/example/pod-list",
            "component": {
                "$codeRef": "PodList"
            }
        }
    },
    {
        "type": "console.page/route",
        "properties": {
            "path": "/example/pod-details/:ns/:name",
            "component": {
                "$codeRef": "PodDetails"
            }
        }
    },
    {
        "type": "console.navigation/href",
        "properties": {
            "id": "pod-list-nav",
            "perspective": "admin",
            "section": "example-section",
            "name": "Pod List",
            "href": "/example/pod-list"
        }
    }
]