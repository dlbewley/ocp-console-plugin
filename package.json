{
    "name": "ocp-console-plugin",
    "version": "0.0.1",
    "private": true,
    "repository": "git@github.com:dlbewley/ocp-console-plugin.git",
    "scripts": {
        "clean": "rm -rf dist",
        "build": "npm run clean && NODE_ENV=production npm run ts-node node_modules/.bin/webpack",
        "build:dev": "npm run clean && NODE_ENV=development npm run ts-node node_modules/.bin/webpack",
        "start": "npm run ts-node node_modules/.bin/webpack serve",
        "start:dev": "npm run ts-node node_modules/.bin/webpack serve",
        "ts-node": "ts-node -O '{\"module\":\"commonjs\"}'",
        "lint": "eslint ./src --fix",
        "test": "jest"
    },
    "consolePlugin": {
        "name": "ocp-console-plugin",
        "version": "0.0.1",
        "displayName": "OpenShift Console Plugin Example",
        "description": "An example OpenShift console plugin",
        "exposedModules": {
            "ExamplePage": "./components/ExamplePage",
            "PodList": "./components/PodList",
            "PodDetails": "./components/PodDetails",
            "NodeNetworkStateList": "./components/NodeNetworkStateList",
            "NodeNetworkStateDetails": "./components/NodeNetworkStateDetails"
        },
        "dependencies": {
            "@console/pluginAPI": "*"
        }
    },
    "dependencies": {
        "@patternfly/react-core": "^6.0.0",
        "@patternfly/react-icons": "^6.0.0",
        "@patternfly/react-table": "^6.0.0",
        "@types/dagre": "^0.7.53",
        "dagre": "^0.8.5",
        "i18next": "^20.6.1",
        "react": "^17.0.1",
        "react-dom": "^17.0.1",
        "react-helmet": "^6.1.0",
        "react-i18next": "^11.8.11",
        "react-router-dom": "^5.3.0",
        "reactflow": "^11.11.4"
    },
    "devDependencies": {
        "@openshift-console/dynamic-plugin-sdk": "^1.0.0",
        "@openshift-console/dynamic-plugin-sdk-webpack": "^1.0.0",
        "@types/node": "^18.0.0",
        "@types/react": "^17.0.37",
        "@types/react-dom": "^17.0.11",
        "@types/react-helmet": "^6.1.0",
        "@types/react-router-dom": "^5.3.2",
        "@types/webpack": "^5.28.0",
        "@types/webpack-dev-server": "^4.7.2",
        "css-loader": "^6.5.1",
        "style-loader": "^3.3.1",
        "ts-loader": "^9.2.6",
        "ts-node": "^10.4.0",
        "typescript": "^5.9.3",
        "webpack": "^5.65.0",
        "webpack-cli": "^4.9.1",
        "webpack-dev-server": "^4.7.1"
    }
}
